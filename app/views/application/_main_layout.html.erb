    <div class="container-fluid">
      <div class="row-fluid">
        <div class="span2">
        </div><!--/span-->
        <div class="span7">

          <div class="navbar navbar-fluid-top">
            <div class="navbar-inner">
              <a class="btn btn-navbar" data-target=".nav-collapse" data-toggle="collapse">
                <span class="icon-bar"></span>
                <span class="icon-bar"></span>
                <span class="icon-bar"></span>
              </a>
              <%= link_to Learnery::Theme::SITE_NAME, root_path, class: "brand" %>
              <div class="container-fluid nav-collapse">
                <ul class="nav">
                  <%= render "application_links" %>
                  <%= render "site_links" %>
                  <%= render "admin_links" %>
                  <%= render 'authentication_links' %>
                </ul>
              </div><!--/.nav-collapse -->
            </div>
          </div><!--/.navbar -->

          <div class="my_wrapper_bottom">
            <%= bootstrap_flash %>
            <%= yield %>
          </div><!--/.my_wrapper_bottom -->

          <div class="my_wrapper_footer">
            <footer>
              <%= render "footer" %>
            </footer>
          </div><!--/.my_wrapper_footer -->
        </div>
        <div class="span2">
           <%= image_tag "banner-1.jpg" %>
           <%= image_tag "banner-2.jpg" %>
           <%= image_tag "banner-3.jpg" %>
           <%= image_tag "banner-4.jpg" %>
        </div><!--/span-->
      </div><!--/row-->


